
{
  "Name": "home2-small",
  "Datacenter": "woodinville",
  "Environment": "development",
  "TimeSources": [ "pool.ntp.org" ],
  "DebugMode": true,
  "AllowUnitTesting": true,
  "Hosting": {
    "Environment": "local-hyper-v",
    "VmDriveFolder": "C:\\VM"
  },
  "Network": {
    "PremiseSubnet": "10.0.0.0/24",
    "NodesSubnet": "10.0.0.0/24"
  },
  "Vpn": {
    "Enabled": false
  },
  "HostNode": {
    "OperatingSystem": "ubuntu-16.04",
    "Upgrade": "none",
    "SshAuth": "tls",
    "PasswordLength": 20
  },
  "Docker": {
    "Version": "18.03.0-ce",
    //"Version": "17.12.0-ce",
    //"Version": "17.09.1-ce",
    //"Version": "17.06.1-ce",
    //"Version": "17.03.2-ce",
    //"Version": "latest",
    //"Version": "test",
    //"Version": "experimental",
    "RegistryCache": true,
    "Experimental": false
  },
  "Vault": {
    "KeyCount": 1,
    "KeyThreshold": 1
  },
  "Log": {
    "Enabled": true
  },
  "Ceph": {
    "Enabled": true
  },
  "Nodes": {
    "manager-0": {
      "Role": "manager",
      "PrivateAddress": "10.0.0.50",
      "Labels": {
        "StorageSSD": true
      }
    },
    "worker-0": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.51",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": true
      }
    },
    "worker-1": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.52",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": false
      }
    },
    "worker-2": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.53",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": false
      }
    },
    "pet-0": {
      "Role": "pet",
      "PrivateAddress": "10.0.0.54",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": false
      }
    }
  }
}

