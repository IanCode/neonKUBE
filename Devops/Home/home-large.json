{
  "Name": "home",
  "Datacenter": "woodinville",
  "Environment": "development",
  "TimeSources": [ "pool.ntp.org" ],
  "Hosting": {
    "ManagerRouterAddress": "lill-home.ddns.net",
    "Machine": {
      "DeployVMs": true
    }
  },
  "Network": {
    "NodesSubnet": "10.0.0.0/24"
  },
  "Vpn": {
    "Enabled": false
  },
  "HostAuth": {
    "SshAuth": "tls",
    "PasswordLength": 20
  },
  "Docker": {
    "Version": "17.09.0-ce",
    //"Version": "17.06.1-ce",
    //"Version": "17.03.2-ce",
    //"Version": "latest",
    //"Version": "test",
    //"Version": "experimental",
    "RegistryCache": true,
    "Experimental": false
  },
  "Vault": {
    "KeyCount": 1,
    "KeyThreshold": 1
  },
  "Log": {
    "Enabled": true
  },
  "Nodes": {
    "manager-0": {
      "Role": "manager",
      "PrivateAddress": "10.0.0.30",
      "Labels": {
        "StorageSSD": true
      }
    },
    "manager-1": {
      "Role": "manager",
      "PrivateAddress": "10.0.0.31",
      "Labels": {
        "StorageSSD": true
      }
    },
    "manager-2": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.32",
      "Labels": {
        "StorageSSD": true
      }
    },
    "worker-0": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.40",
      "Labels": {
        "StorageSSD": true
      }
    },
    "worker-1": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.41",
      "Labels": {
        "StorageSSD": true
      }
    },
    "worker-2": {
      "Role": "worker",
      "PrivateAddress": "10.0.0.42",
      "Labels": {
        "StorageSSD": true
      }
    },
    "pet-0": {
      "Role": "pet",
      "PrivateAddress": "10.0.0.43",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": true
      }
    },
    "pet-1": {
      "Role": "pet",
      "PrivateAddress": "10.0.0.44",
      "Labels": {
        "StorageSSD": true,
        "LogEsData": true
      }
    }
  }
}
